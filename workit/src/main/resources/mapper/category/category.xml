<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.magis.workit.mapper.category.CategoryMapper">
    <select id="getCateList" resultType="com.magis.workit.dto.category.CategoryDto">
        select *
        from category
    </select>

    <insert id="createCate">
        Insert into category (cateName)
        values #{cateName}
    </insert>

    <update id="modifyCate">
        update category
        set cateName = #{cateName}
        where cateId = #{cateId}
    </update>

    <delete id="deleteCate">
        delete from category
        where cateid = #{cateId}
    </delete>

</mapper>
